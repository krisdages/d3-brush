{
  "name": "@krisdages/d3-brush",
  "version": "2.1.0-key-modifiers.0",
  "description": "Select a one- or two-dimensional region using the mouse or touch.",
  "keywords": [
    "d3",
    "d3-module",
    "brush",
    "interaction"
  ],
  "homepage": "https://d3js.org/d3-brush/",
  "license": "BSD-3-Clause",
  "author": {
    "name": "Mike Bostock",
    "url": "http://bost.ocks.org/mike"
  },
  "contributors": ["Kris Dages"],
  "main": "dist/d3-brush.js",
  "unpkg": "dist/d3-brush.min.js",
  "jsdelivr": "dist/d3-brush.min.js",
  "module": "src/index.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/krisdages/d3-brush.git"
  },
  "files": [
    "dist/**/*.js",
    "src/**/*.js"
  ],
  "scripts": {
    "pretest": "rollup -c",
    "test": "tape 'test/**/*-test.js' && eslint src",
    "prepublishOnly": "rm -rf dist && yarn test"
  },
  "dependencies": {
    "d3-dispatch": "1 - 2",
    "d3-drag": "2",
    "d3-interpolate": "1 - 2",
    "d3-selection": "2",
    "d3-transition": "2"
  },
  "devDependencies": {
    "eslint": "6",
    "rollup": "1",
    "rollup-plugin-terser": "5",
    "tape": "4"
  }
}
